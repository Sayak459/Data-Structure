#include<stdio.h>
#include<stdlib.h>
 void display(int arr[],int n)
 {
 	for(int i=0;i<n;i++)
 	{
 		printf("%d \n",arr[i]);
 	}
 }
 int indinsertion(int arr[],int size,int capacity,int element,int index)
 {
 	if(size>=capacity)
 	{
 		return -1;
 	}
 	for(int i=size-1;i<=index;i--)
 	{
 		arr[i+1]=arr[i];
 	}
 	arr[index]=element;
 }
 return 1;
 
 int main()
 {
 	arr[50]={23,54,23,66,7}
 	int size=5;
 	display(arr,index);
 	int element 46;
 	int index 3;
 	display(arr,index);
 	size+=1;
 }
 				
 	
